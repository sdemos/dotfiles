set incsearch
set number
set showmatch
set shiftwidth=3
set autoindent
set scrolloff=5
set ignorecase
set smartcase
set wildmode=longest,list
set nosmartindent
set list
set nocompatible
set showcmd
set ruler
set showmode
set listchars=tab:>-,trail:-,eol:$,nbsp:%,
let python_highlight_all=1
syntax on
inoremap jj <Esc>
source $HOME/.vim/plugin/anotherdark.vim
colorscheme anotherdark
set tabstop=4
set shiftwidth=4
set expandtab

