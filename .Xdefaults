! .Xdefaults
! author: Stephen Demos

! change term name
urxvt*termName: rxvt-unicode-256color

! transparency - true or false (default)
urxvt*transparent: false

! tint with any color
!	some nice listings are at:
!		http://www.nisrv.com/drupal/?q=node/11
!		http://htmlgoodies.com/tutorials/colors/article.php/3478921
! urxvt*tintColor: Blue

! shading - 0 to 99 darkens, 101 to 200 lightens
!	Dont use with tintColor; just use a darker or lighter color instead
urxvt*shading: 40

! scrolback buffer lines - 65535 is max on most machines (64 is default)
urxvt*saveLines: 4096

! font color (default is black)
urxvt*foreground: White

! background color (prior to tinting) (default is white)
urxvt*background: #080808

! fonts for urxvt
urxvt*font: xft:inconsolata:pixelsize=13:antialias=false:hinting=true
! this gets rid of bold fonts
urxvt*boldFont:

! fuck the scroll bar
urxvt*scrollBar: false

! make the lines save for a while
urxvt*saveLines: 4095

urxvt*color0:       #3f3f3f
urxvt*color1:       #903030
urxvt*color2:       #00b45a
urxvt*color3:       #bfbf00
urxvt*color4:       #2090e0
urxvt*color5:       #dc8cc3
urxvt*color6:       #60c0c0
urxvt*color7:       #ececec
urxvt*color8:       #709080
urxvt*color9:       #e04040
urxvt*color10:      #4def4d
urxvt*color11:      #f0df8f
urxvt*color12:      #4186de
urxvt*color13:      #ec93d3
urxvt*color14:      #93e0e3
urxvt*color15:      #ffffff
